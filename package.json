{
    "name": "appengine-typescript",
    "description": "Express API",
    "version": "0.0.1",
    "private": true,
    "license": "Owned by Polydelic",
    "author": "Oliver Dixon | Polydelic AS",
    "scripts": {
        "prepare": "npm run gcp-build",
        "build": "cross-env tsc -p .",
        "gcp-build": "tsc -p .",
        "dev": "cross-env NODE_ENV=dev GCP_PROJECT=awake-d48d9 nodemon",
        "start": "NODE_ENV=production node ./dist/index.js",
        "build-docs": "cross-env rm -rf ./docs && mkdir ./docs && touch ./docs/index && apidoc -f .*\\.ts$ -i src/ -o docs/",
        "test": "ava",
        "docker-deploy-dev": "npm run build-docs && docker build --platform linux/amd64 -t awake-image . && docker tag awake-image eu.gcr.io/awake-d48d9/awake-image && docker push eu.gcr.io/awake-d48d9/awake-image && y | gcloud app deploy --project awake-d48d9 app.yaml --image-url=eu.gcr.io/awake-d48d9/awake-image",
        "docker-deploy-prod": "npm run build-docs && docker build --platform linux/amd64 -t awake-image . && docker tag awake-image eu.gcr.io/awake-prod/awake-image && docker push eu.gcr.io/awake-prod/awake-image && y | gcloud app deploy --project awake-prod app.yaml --image-url=eu.gcr.io/awake-prod/awake-image",
        "docker-run": "cross-env docker run --platform linux/amd64 -it -p 3000:8080 awake-image",
        "docker-setup": "cross-env gcloud auth configure-docker --project awake-d48d9",
        "create-dummy": "ava --match='create-dummy-site - Classic cars and health'"
    },
    "dependencies": {
        "@google-cloud/language": "^4.2.3",
        "@google-cloud/logging-winston": "^4.1.0",
        "@google-cloud/secret-manager": "^3.8.0",
        "@google-cloud/storage": "^5.8.4",
        "@types/bcrypt": "^3.0.0",
        "@types/express-rate-limit": "^5.1.1",
        "@types/joi": "^17.2.3",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/jwt-then": "^1.0.0",
        "@types/mongodb": "^3.6.8",
        "@types/multer": "^1.4.5",
        "@types/nodemailer": "^6.4.1",
        "@types/showdown": "^1.9.3",
        "@types/spotify-api": "0.0.4",
        "@types/supertest": "^2.0.10",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "express": "4.16.3",
        "express-promise-router": "3.0.3",
        "express-rate-limit": "^5.2.6",
        "google-play-billing-validator": "2.1.1",
        "install": "^0.13.0",
        "joi": "^17.4.0",
        "jwt-then": "^1.0.1",
        "lorem-ipsum": "^2.0.3",
        "mongodb": "^3.6.5",
        "multer": "^1.4.2",
        "nexmo": "^2.9.1",
        "nodemailer": "^6.5.0",
        "npm": "^7.6.0",
        "querystring": "^0.2.0",
        "random-picture": "^4.0.3",
        "require_optional": "^1.0.1",
        "sharp": "^0.28.3",
        "showdown": "^1.9.1",
        "supertest": "^4.0.2",
        "ts-node": "8.4.1",
        "unique-names-generator": "^4.4.0",
        "url": "^0.11.0",
        "util": "^0.12.3",
        "winston": "3.2.1"
    },
    "devDependencies": {
        "@ava/typescript": "^1.1.1",
        "@google-cloud/nodejs-repo-tools": "^1.0.8",
        "@types/express": "4.16.0",
        "apidoc": "^0.20.1",
        "ava": "^3.15.0",
        "nodemon": "2.0.2",
        "typescript": "3.8.3"
    }
}
